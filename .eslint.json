{
    "extends":"airbnb",
    "parser":"babel-eslint",
    "parserOptions":{
        "ecmaVersion":7,
        "sourceType":"module",
        "ecmaFeatures":{
            "jsx":true
        }
    },
    "rules":{
        "strict":0,
        "semi":2,
        "indent":[
            2,
            4
        ],
        "max-len":0,
        "no-extend-native":1,
        "new-cap":1,
        "react/jsx-indent":[
            2,
            4
        ],
        "react/prefer-stateless-function":1,
        "react/jsx-no-bind":1,
        "camelcase":0,
        "no-underscore-dangle":0
    }
}
